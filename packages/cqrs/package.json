{
  "name": "mr-cqrs",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --emitDeclarationOnly",
    "build": "npm run clean && vite build && npm run typecheck",
    "dev": "vite",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --watch --config vitest.config.ts"
  },
  "dependencies": {
    "@mr/pipeline-core": "*",
    "@mr/design-patterns": "*",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "vitest": "^2.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.0"
  }
}